[
    //
    // Editing...
    //
    // Refactoring
    { "key": "alt+r alt+r", "command": "editor.action.changeAll", "when": "editorTextFocus && !editorReadonly" },
    /* remove */ { "key": "ctrl+f2", "command": "-editor.action.changeAll", "when": "editorTextFocus && !editorReadonly" },
    // Navigate
    //   @TODO(stdmatt): The navigation with vscode and vim are totally broken
    //   it is actually getting all the keystrokes related with navigation
    //   like up, down, etc.. and using it to get back...
    { "key": "ctrl+-", "command": "workbench.action.navigateBack"    },
    { "key": "ctrl+=", "command": "workbench.action.navigateForward" },
    // Fold
    { "key": "ctrl+,", "command": "editor.fold" },
    { "key": "ctrl+.", "command": "editor.unfold" },
    /* remove */ { "key": "ctrl+shift+[", "command": "-editor.fold", "when": "editorTextFocus && foldingEnabled" },
    /* remove */ { "key": "ctrl+shift+]", "command": "-editor.unfold", "when": "editorTextFocus && foldingEnabled" },
    // Comments
    { "key": "alt+/",       "command": "editor.action.commentLine", "when": "editorTextFocus && !editorReadonly" },
    { "key": "alt+k alt+c", "command": "editor.action.commentLine", "when": "editorTextFocus && !editorReadonly" },
    /* remove */ { "key": "ctrl+/", "command": "-editor.action.commentLine", "when": "editorTextFocus && !editorReadonly" },
    /* remove */ { "key": "ctrl+k ctrl+c", "command": "-editor.action.addCommentLine", "when": "editorTextFocus && !editorReadonly" },
    // Duplicate line
    { "key": "alt+j alt+j", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus && !editorReadonly" },
    { "key": "alt+k alt+k", "command": "editor.action.copyLinesUpAction",   "when": "editorTextFocus && !editorReadonly" },
    /* remove */ { "key": "shift+alt+down", "command": "-editor.action.copyLinesDownAction", "when": "editorTextFocus && !editorReadonly" },
    /* remove */ { "key": "shift+alt+up",   "command": "-editor.action.copyLinesUpAction",   "when": "editorTextFocus && !editorReadonly" },
    // Clipboard
    { "key": "ctrl+shift+c",  "command": "editor.action.clipboardCopyAction",  "when": "textInputFocus" },
    { "key": "ctrl+shift+v",  "command": "editor.action.clipboardPasteAction", "when": "textInputFocus" },
    // Suggestions
    { "key": "ctrl+j", "command": "selectNextSuggestion", "when": "suggestWidgetVisible" },
    { "key": "ctrl+k", "command": "selectPrevSuggestion", "when": "suggestWidgetVisible" },
    // Find
    { "key": "ctrl+f ctrl+f", "command": "actions.find" },
    { "key": "ctrl+f ctrl+h", "command": "workbench.action.findInFiles" },
    // Insert date time...
    { "key": "alt+;", "command": "extension.insertDateTime" },

    //
    // Editor
    //--------------------------------------------------------------------------
    // Maximize / Minimize Others
    //   @todo(stdmatt): This is very similar to the behaviour of the panes
    //   should we keep it here or move to the panes section???
    { "key": "ctrl+e ctrl+m",             "command": "workbench.action.maximizeEditor"       },
    { "key": "shift+ctrl+e shift+ctrl+m", "command": "workbench.action.minimizeOtherEditors" },
    // Toggle the surrounding panels...
    { "key": "shift+alt+t", "command": "workbench.action.togglePanel" },
    { "key": "shift+alt+m", "command": "editor.action.toggleMinimap" },
    { "key": "alt+shift+e", "command": "workbench.action.toggleSidebarVisibility" },
    { "key": "ctrl+shift+e",     "command": "workbench.action.focusSideBar" },
    { "key": "ctrl+shift+alt+l", "command": "workbench.action.focusNextPart" },
    { "key": "ctrl+shift+alt+h", "command": "workbench.action.focusPreviousPart" },

    //
    // Quick Open
    //
    //--------------------------------------------------------------------------
    { "key": "alt+shift+o", "command": "workbench.action.quickOpen"    },
    { "key": "alt+shift+p", "command": "workbench.action.showCommands" },
    /* remove */ { "key": "ctrl+e",         "command": "-workbench.action.quickOpen" },
    /* remove */ { "key": "ctrl+p",         "command": "-workbench.action.quickOpen" },
    /* remove */ { "key": "ctrl+shift+tab", "command": "-workbench.action.quickOpenLeastRecentlyUsedEditorInGroup" },
    /* remove */ { "key": "ctrl+tab",       "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup" },
    /* remove */ { "key": "ctrl+q",         "command": "-workbench.action.quickOpenView" },
    { "key": "ctrl+n", "command": "workbench.action.quickOpenSelectNext",     "when": "inQuickOpen" },
    { "key": "ctrl+p", "command": "workbench.action.quickOpenSelectPrevious", "when": "inQuickOpen" },
    { "key": "ctrl+j", "command": "workbench.action.quickOpenSelectNext",     "when": "inQuickOpen" },
    { "key": "ctrl+k", "command": "workbench.action.quickOpenSelectPrevious", "when": "inQuickOpen" },


    //
    // Panes
    //
    //--------------------------------------------------------------------------
    // Split
    { "key": "ctrl+shift+\\", "command": "workbench.action.splitEditor"          },
    { "key": "ctrl+shift+-",  "command": "workbench.action.splitEditorOrthogonal"},
    // Focus
    { "key": "alt+h", "command": "workbench.action.focusLeftGroup" , "when": "!inQuickOpen"},
    { "key": "alt+l", "command": "workbench.action.focusRightGroup", "when": "!inQuickOpen"},
    // @todo(stdmatt): Conflicts with comment...
    { "key": "alt+k", "command": "workbench.action.focusAboveGroup", "when": "!inQuickOpen"},
    { "key": "alt+j", "command": "workbench.action.focusBelowGroup", "when": "!inQuickOpen"},
    // Move
    { "key": "alt+shift+h", "command": "workbench.action.moveEditorToLeftGroup"  },
    { "key": "alt+shift+l", "command": "workbench.action.moveEditorToRightGroup" },
    // Size
    { "key": "ctrl+shift+h", "command": "workbench.action.increaseViewWidth" },
    { "key": "ctrl+shift+l", "command": "workbench.action.decreaseViewWidth" },
    { "key": "ctrl+shift+j", "command": "workbench.action.increaseViewHeight" },
    { "key": "ctrl+shift+k", "command": "workbench.action.decreaseViewHeight" },
    // Close
    { "key": "shift+alt+x", "command": "workbench.action.closePanel" },
    { "key": "shift+alt+x", "command": "workbench.action.closeActiveEditor" },
    // Panel
    { "key": "alt+t alt+l", "command": "workbench.action.nextPanelView" },
    { "key": "alt+t alt+h", "command": "workbench.action.previousPanelView" },
    { "key": "alt+t alt+t", "command": "workbench.action.togglePanel" },
    // List
    { "key": "ctrl+h", "command": "list.collapse",  "when": "listFocus && !inputFocus" },
    { "key": "ctrl+l", "command": "list.expand",    "when": "listFocus && !inputFocus" },
    { "key": "ctrl+k", "command": "list.focusUp",   "when": "listFocus && !inputFocus" },
    { "key": "ctrl+j", "command": "list.focusDown", "when": "listFocus && !inputFocus" },

    //
    // Tabs
    //
    //--------------------------------------------------------------------------
    // Move
    { "key": "ctrl+tab",       "command": "workbench.action.nextEditor"     },
    { "key": "ctrl+shift+tab", "command": "workbench.action.previousEditor" },
    { "key": "ctrl+alt+l",     "command": "workbench.action.nextEditor"     },
    { "key": "ctrl+alt+h",     "command": "workbench.action.previousEditor" },
    // Pin.
    { "key": "ctrl+e ctrl+p", "command": "workbench.action.pinEditor",   "when": "!activeEditorIsPinned" },
    { "key": "ctrl+e ctrl+p", "command": "workbench.action.unpinEditor", "when": "activeEditorIsPinned" },

    //
    //
    // Sync
    //--------------------------------------------------------------------------
    { "key": "ctrl+shift+f11 ctrl+shift+f12", "command": "workbench.userDataSync.actions.syncNow" },


    //
    // Disable stuff...
    //
    //--------------------------------------------------------------------------
    /* remove */ { "key": "alt+c", "command": "-toggleFindCaseSensitive",   "when": "editorFocus" },
    /* remove */ { "key": "alt+c", "command": "-toggleSearchCaseSensitive", "when": "searchViewletFocus" },
    /* remove */ { "key": "alt+c", "command": "-toggleSearchEditorCaseSensitive", "when": "inSearchEditor && searchInputBoxFocus" },
    /* remove */ { "key": "alt+c", "command": "-workbench.action.terminal.toggleFindCaseSensitive", "when": "terminalFindFocused && terminalProcessSupported || terminalFocus && terminalProcessSupported" },
    /* remove */ { "key": "ctrl+j", "command": "-workbench.action.togglePanel" },
    /* remove */ { "key": "ctrl+f4", "command": "-workbench.action.terminal.killEditor", "when": "terminalEditorFocus && terminalFocus && terminalProcessSupported && resourceScheme == 'vscode-terminal'" },
    /* remove */ { "key": "ctrl+shift+t", "command": "-workbench.action.reopenClosedEditor" },
    /* remove */ { "key": "ctrl+shift+e", "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker", "when": "inFilesPicker && inQuickOpen" },
    /* remove */ { "key": "ctrl+shift+e", "command": "-workbench.view.explorer", "when": "viewContainer.workbench.view.explorer.enabled" },
    /* remove */ { "key": "ctrl+0", "command": "-workbench.action.focusSideBar" },
    /* remove */ { "key": "f6", "command": "-workbench.action.focusNextPart" },
    /* remove */ {
        "key": "shift+f6",
        "command": "-workbench.action.focusPreviousPart"
    },
    {
        "key": "ctrl+shift+alt+,",
        "command": "editor.foldAll",
        "when": "editorTextFocus && foldingEnabled"
    },
    {
        "key": "ctrl+k ctrl+0",
        "command": "-editor.foldAll",
        "when": "editorTextFocus && foldingEnabled"
    }
]

