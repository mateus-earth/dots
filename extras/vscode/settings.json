    //
    // Debugger
    //
    //--------------------------------------------------------------------------
    "debug.onTaskErrors":    "abort",
    "debug.toolBarLocation": "docked",

    //
    // Editor
    //
    //--------------------------------------------------------------------------
    "editor.codeLens":                          false,
    "editor.renderWhitespace":                  "all",
    "editor.acceptSuggestionOnEnter":           "smart",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.minimap.renderCharacters":          false,

    "editor.fontFamily":    "Jetbrains Mono Light",
    "editor.fontSize":      15,
    "editor.fontLigatures": true,
    "editor.rulers":        [ 80, 120 ],

    //
    // Explorer
    //
    //--------------------------------------------------------------------------
    "explorer.confirmDelete":             false,
    "explorer.confirmDragAndDrop":        false,
    "explorer.autoReveal":                false,
    "explorer.compactFolders":            false,
    "explorer.copyRelativePathSeparator": "/",
    "explorer.openEditors.sortOrder":     "alphabetical",
    "explorer.openEditors.visible":       0,

    //
    // Extensions
    //
    //--------------------------------------------------------------------------
    "extensions.autoUpdate":       false,
    "extensions.autoCheckUpdates": true,
        // Auto Align
        "alignment.surroundSpace" : {
            "colon":      [-1, 1],  // The first number specify how much space to add to the left, can be negative.
                                    // The second number is how much space to the right, can be negative.
            "assignment": [1, 1],   // The same as above.
            "arrow":      [1, 1],   // The same as above.
            "comment":    2         // Special how much space to add between the trailing comment and the code.
                                    // If this value is negative, it means don't align the trailing comment.
        },
        // Bracket Pair Colorizer
        "bracket-pair-colorizer-2.colorMode":               "Consecutive",
        "bracket-pair-colorizer-2.forceUniqueOpeningColor": false,
        "bracket-pair-colorizer-2.showBracketsInGutter":    true,
        "bracket-pair-colorizer-2.highlightActiveScope":    true,
        // Peacock
        "peacock.affectEditorGroupBorder":     false,
        "peacock.affectDebuggingStatusBar":    false,
        "peacock.affectPanelBorder":           false,
        "peacock.affectSideBarBorder":         false,
        "peacock.affectTabActiveBorder":       false,
        "peacock.affectStatusBar":             false,
        "peacock.affectStatusAndTitleBorders": true,
        // NeoVim
        "vscode-neovim.neovimExecutablePaths.win32": "C:\\Neovim\\bin\\nvim.exe",
        // Todo-Tree
        "todo-tree.regex.enableMultiLine":    true,
        "todo-tree.regex.regexCaseSensitive": false,
        "todo-tree.regex.subTagRegex":        "^\\s*\\((.*)\\)",
        "todo-tree.highlights.customHighlight": {
            "@todo": {
                "type":       "text",

                "foreground": "#FFD700",
                "iconColour": "#FFD700",
                "opacity": 0,

                "gutterIcon": true,
                "icon":       "hubot",
            },

            "@notice": {
                "type":       "text",

                "foreground": "#5396CE",
                "iconColour": "#5396CE",
                "opacity": 0,

                "gutterIcon": true,
                "icon":       "smiley",
            },

            "@XXX": {
                "type":       "text",

                "foreground": "#D65656",
                "iconColour": "#D65656",
                "opacity": 0,

                "gutterIcon": true,
                "icon":       "bug",
            }
        },
        "todo-tree.general.tags": [
            "@XXX",
            "@notice",
            "@todo"
        ],

    //
    // Files
    //
    //--------------------------------------------------------------------------
    "files.eol":                    "\n",
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines":      true,
    "files.insertFinalNewline":     false,

    "files.associations": {
        "*.mold":    "cpp",
        ".ark_bump": "json"
    },

    "files.exclude": {
        "**/__pycache__": true,
        "**/*.fla":       true,
        "**/*.swf":       true
    },


    //
    // SCM
    //
    //--------------------------------------------------------------------------
    "scm.autoReveal": false,
    "scm.countBadge": "off",

    //
    // Search
    //
    //--------------------------------------------------------------------------
    "search.useReplacePreview": false,

    //
    // Telemetry
    //
    //--------------------------------------------------------------------------
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry":     false,
    "telemetry.telemetryLevel":      "off",

    //
    // Update
    //
    //--------------------------------------------------------------------------
    "update.enableWindowsBackgroundUpdates":          false,
    "update.mode":                                    "manual",
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.tree.renderIndentGuides":              "always",
    "workbench.editor.enablePreviewFromQuickOpen":    false,
    "workbench.editor.enablePreview":                 false,
    "workbench.startupEditor":                        "none",
    "workbench.editor.limit.value":                   20,
    "workbench.editor.tabSizing":                     "shrink",
    "workbench.colorTheme":                           "Visual Assist Dark Theme",

    //
    // Window
    //
    //--------------------------------------------------------------------------
    "window.title":                  "${dirty}[${activeEditorShort}] ${separator} ${activeEditorMedium} ${separator} ${rootName}",
    "window.customMenuBarAltFocus":  false,
    "window.zoomLevel":              1,
    "window.dialogStyle":            "custom",
    "window.enableMenuBarMnemonics": false,
    // Python
    "python.linting.pylintEnabled": false,
    "python.showStartPage":         false,
    // C#
    "csharp.suppressDotnetInstallWarning": true,
    "csharp.testsCodeLens.enabled":        false,
    "csharp.referencesCodeLens.enabled":   false,
    // Powershell
    "powershell.buttons.showRunButtons":                   false,
    "powershell.codeFolding.showLastLine":                 false,
    "powershell.codeFormatting.addWhitespaceAroundPipe":   false,
    "powershell.codeFormatting.alignPropertyValuePairs":   false,
    "powershell.codeFormatting.newLineAfterCloseBrace":    false,
    "powershell.codeFormatting.newLineAfterOpenBrace":     false,
    "powershell.codeFormatting.openBraceOnSameLine":       false,
    "powershell.codeFormatting.pipelineIndentationStyle": "None",
    "powershell.codeFormatting.whitespaceAfterSeparator":  false,
    "powershell.codeFormatting.whitespaceAroundOperator":  false,
    "powershell.codeFormatting.whitespaceAroundPipe":      false,
    "powershell.codeFormatting.whitespaceBeforeOpenBrace": false,
    "powershell.codeFormatting.whitespaceBeforeOpenParen": false,
    // Javascript
    "javascript.format.enable":                               false,
    "javascript.inlayHints.functionLikeReturnTypes.enabled":  true,
    "javascript.inlayHints.parameterTypes.enabled":           false,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": false,
    "javascript.inlayHints.variableTypes.enabled":            false,
    "javascript.preferences.quoteStyle":                      "double",
    // Others
    "notebook.cellToolbarLocation": {
        "default":          "right",
        "jupyter-notebook": "left"
    },

    //
    // Security
    //
    //--------------------------------------------------------------------------
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.enabled":        false,

    // Update
    //--------------------------------------------------------------------------
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "powershell.promptToUpdatePowerShell":          false,


    //
    // stdmatt
    //
    "stdmatt_vscode.editor_colors": {
        "test": "#FF00FF",
        "tools": "#696c9d"
    },

    "stdmatt_vscode.snippets": {
        "_todo":[
            "@todo(stdmatt): [${1:TITLE}] - $CURRENT_DATE $CURRENT_MONTH_NAME_SHORT, $CURRENT_YEAR at $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
            "${2}"
        ],
        "_notice":[
            "@notice(stdmatt): [${1:TITLE}] - $CURRENT_DATE $CURRENT_MONTH_NAME_SHORT, $CURRENT_YEAR at $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
            "${2}"
        ],
        "_XXX":[
            "@XXX(stdmatt): [${1:TITLE}] - $CURRENT_DATE $CURRENT_MONTH_NAME_SHORT, $CURRENT_YEAR at $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND",
            "${2}"
        ],
    },

